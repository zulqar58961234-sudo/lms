
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @map("_id") @default(auto()) @db.ObjectId
  name     String
  email    String @unique
  password String
}
model Studentinfo {
  id   String       @id      @default(auto())     @map("_id")     @db.ObjectId
  name String
  age  Int
  course String
  image String
  createdAt DateTime @default(now())
}
